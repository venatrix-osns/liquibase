<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- ChangeSet 1: Create the table -->
    <changeSet id="1" author="VENATRIX">
        <sqlFile path="sql/create_table.sql" />
    </changeSet>

        <!-- ChangeSet 2: Insert Column after the table is created -->
    <changeSet id="2" author="SADJITH">
        <sqlFile path="sql/add_column.sql" />
    </changeSet>

    <!-- ChangeSet 4: Insert initial data -->
    <changeSet id="4" author="venatrix">
        <sqlFile path="sql/insert_data.sql" />
    </changeSet>

    <!-- ChangeSet 5: Update existing records -->
    <changeSet id="5" author="sadjith">
        <sqlFile path="sql/update_records.sql" />
    </changeSet>

    <!-- ChangeSet 6: Drop a column from a table -->
    <changeSet id="6" author="sadjith">
        <sqlFile path="sql/drop_column.sql" />
    </changeSet>


</databaseChangeLog>
