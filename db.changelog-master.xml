<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd">

    <!-- ChangeSet 1: Create the table -->
    <changeSet id="1" author="VENATRIX">
        <sqlFile path="sql/create_table.sql" />
    </changeSet>

    <!-- ChangeSet 2: Crete coloumn -->
    <changeSet id="2" author="VENATRIX">
        <sqlFile path="sql/add_column.sql" />
    </changeSet>

    <changeSet id="3" author="your-name" runAlways="true">
    <sql>
        INSERT INTO example_table (id, name, age)
        VALUES (1, 'Alice', 30),
               (2, 'Bob', 25);
    </sql>
</changeSet>
    <!-- ChangeSet 4: Update existing records -->
    <changeSet id="4" author="sadjith">
        <sqlFile path="sql/update_records.sql" />
    </changeSet>
    
</databaseChangeLog>
